{
    "manifest_version": 3,
    "name": "Roblox ping checker",
    "descripton": "Ping checker for roblox.",
    "version": "1.0.0",
    "icons": {"128": "roblox_128x128.png"},
    "browser_action": {
        "default_icon": "roblox_19x19.png"
    },
    "content_scripts": [{
        "matches": ["https://www.roblox.com/*"],
        "js": ["content.js"]
    }],
    "host_permissions": ["https://www.roblox.com/*"],
    "permissions": ["activeTab"]
}